def find_factors(number):
    # List to store the factors
    factors = []
    
    # Loop through numbers from 1 to the given number (inclusive)
    for i in range(1, number + 1):
        if number % i == 0:  # If there's no remainder, i is a factor
            factors.append(i)
    
    return factors

# Example usage:
num = 15
print(f"Factors of {num}: {find_factors(num)}")
