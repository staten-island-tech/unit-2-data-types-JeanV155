
def find_gcf(a, b):
    min_value = a if a < b else b  # Find the smaller of the two numbers
    gcf = 1  # Start with the smallest possible GCF

    for i in range(1, min_value + 1):  # Check all numbers up to the smallest number
        if a % i == 0 and b % i == 0:  # If `i` is a common factor
            gcf = i  # Update the GCF
    
    return gcf  # Return the greatest common factor

def find_greatest_common_factors(numbers):
    gcf = numbers[0]
    
    for number in numbers[1:]:
        gcf = find_gcf(gcf, number)  # Find GCF iteratively
        
    return gcf 

numbers = [100, 25]

# Call the function with the numbers list
results = find_greatest_common_factors(numbers)

# Print the result
print(results)  
