
def find_factors(number):
    # List to store the factors
    factors = []
    
    # Loop through numbers from 1 to the given number (inclusive)
    for i in range(1, number + 1):
        if number % i == 0:  # If there's no remainder, i is a factor
            factors.append(i)
    
    return factors

# Example usage:
a = int(input("Enter the first number: "))  # Get the first number from the user
b = int(input("Enter the second number: "))  # Get the second number from the user

# Call the find_gcf function and display the result
factors = find_factors(a, b)
print(f"The  common factor of {a} and {b} is {factors}")
